   "source": [
    "import numpy as np\n",
    "from numpy.linalg import inv"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "id": "7154ee62",
   "metadata": {},
   "outputs": [],
   "source": [
    "y = np.array([[2], [4], [6], [8], [10]])\n",
    "xt = np.array([[1, 1, 1, 1, 1], [5, 4, 3, 2, 1]])\n",
    "x = xt.transpose()\n",
    "temp = np.dot(inv(np.dot(xt, x)), xt)\n",
    "theta = np.dot(temp, y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "4dc0bca7",
   "metadata": {},
