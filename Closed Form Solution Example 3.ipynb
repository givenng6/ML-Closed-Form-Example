   "source": [
    "import numpy as np\n",
    "from numpy.linalg import inv"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "60d432ab",
   "metadata": {},
   "outputs": [],
   "source": [
    "y = np.array([[2], [5], [8], [14], [23]])\n",
    "xt = np.array([ [1, 1, 1, 1, 1], [2, 4, 6, 8, 10]])\n",
    "x = xt.transpose()\n",
    "temp = np.dot(inv(np.dot(xt, x)), xt)\n",
    "theta = np.dot(temp, y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "8841e46c",
